						<marker v="0 db" ox="1" oy="0" width="2" color="graph_prim"/>

						<!-- Meshes -->
						<mesh id="cfc" x.index="2" y.index="3" width="3" color="graph_mesh" smooth="true"/>
						<dot x.id="cfx_l" y.id="cfy_l" color="left" bcolor="left" visibility=":oclip and :ce"/>
						<dot x.id="cfx_r" y.id="cfy_r" color="right" bcolor="right" visibility=":oclip and :ce"/>

						<!-- Markers -->
						<marker id="ct" color="attack_1" width="2" editable="true"/>

						<!-- Text -->
						<text x="0 db" y="0 db" text="0" halign="1" valign="1"/>
						<ui:with x="0 db" halign="1" valign="-1">
							<text y="12 db" text="graph.units.db"/>
							<text y="6 db" text="+6"/>
							<text y="-6 db" text="-6"/>
							<text y="-12 db" text="-12"/>
							<text y="-18 db" text="-18"/>
							<text y="-24 db" text="-24"/>
							<text y="-30 db" text="-30"/>
							<text y="-36 db" text="graph.axis.out" valign="1"/>
						</ui:with>
						<ui:with y="0 db" halign="-1" valign="1">
							<text x="12 db" text="graph.units.db"/>
							<text x="6 db" text="+6"/>
							<text x="-6 db" text="-6"/>
							<text x="-12 db" text="-12"/>
							<text x="-18 db" text="-18"/>
							<text x="-24 db" text="-24"/>
							<text x="-30 db" text="-30"/>
							<text x="-36 db" text="graph.axis.in" halign="1"/>
						</ui:with>

						<text x=":ct" y="12 db" text="graph.values.x::f" text:value=":ct" text.eval="true" color="attack_1" halign="(:ct lt -12 db) ? 1 : -1" valign="-1"/>
						<text x=":ct" y="-36 db" text="graph.values.x:db:f" text:value="20 * lg :ct" text.eval="true" color="attack_1" halign="(:ct lt -12 db) ? 1 : -1" valign="1"/>
					</graph>

					<!-- Clipper gain reduction meter -->
					<vbox>
						<label text="labels.gain_" padding="4"/>
						<ledmeter vexpand="true" angle="1">
							<ledchannel id="cfr_l" min="-12 db" max="12 db" log="true" balance="0 db" balance.visibility="true" type="peak" activity=":oclip and :ce" value.color="blue"/>
							<ledchannel id="cfr_r" min="-12 db" max="12 db" log="true" balance="0 db" balance.visibility="true" type="peak" activity=":oclip and :ce" value.color="blue"/>
						</ledmeter>
					</vbox>

					<!-- Ocillogram graph -->
					<graph width.min="312" height.min="192" expand="true" fill="true">
						<origin hpos="1" vpos="-1" visible="false"/>

						<!-- Gray grid -->
						<ui:with ox="0" oy="1">
							<marker v="0.5" color="graph_alt_1"/>
							<marker v="1.5" color="graph_alt_1"/>
							<marker v="2.5" color="graph_alt_1"/>
							<marker v="3.5" color="graph_alt_1"/>
							<marker v="4.5" color="graph_alt_1"/>
						</ui:with>

						<!-- Yellow grid -->
						<ui:with ox="0" oy="1">
							<marker v="1" color="graph_sec"/>
							<marker v="2" color="graph_sec"/>
							<marker v="3" color="graph_sec"/>
							<marker v="4" color="graph_sec"/>
						</ui:with>
						<ui:with ox="1" oy="0" color="graph_sec">
							<ui:for id="v" first="-18" last="6" step="6">
								<marker v="${v} db"/>
							</ui:for>
						</ui:with>

						<!-- Meshes -->
						<mesh id="ctg_l" width="0" x.index="0" y.index="1" color="left_in" fcolor="left_in" fcolor.a="0.8" fill="true" visibility=":oclip and :ilg_l"/>
						<mesh id="ctg_r" width="0" x.index="0" y.index="1" color="right_in" fcolor="right_in" fcolor.a="0.8" fill="true" visibility=":oclip and :ilg_r"/>
						<mesh id="ctg_l" width="2" x.index="0" y.index="2" color="left" visibility=":oclip and :olg_l"/>
						<mesh id="ctg_r" width="2" x.index="0" y.index="2" color="right" visibility=":oclip and :olg_r"/>
						<mesh id="ctg_l" width="2" x.index="0" y.index="3" color="bright_blue" visibility=":oclip and :grg_l"/>
						<mesh id="ctg_r" width="2" x.index="0" y.index="3" color="bright_blue" visibility=":oclip and :grg_r"/>

						<!-- Boundaries -->
						<marker v="24 db" ox="1" oy="0" color="graph_prim" visibility="false"/>
						<marker v="5" ox="0" oy="1" color="graph_prim" visibility="false"/>

						<!-- Axis -->
						<axis min="0" max="5" angle="1.0" log="false" color="graph_prim" visibility="false"/>
						<axis min="-24 db" max="12 db" angle="0.5" log="true" color="graph_prim" visibility="false"/>

						<!-- Text -->
						<text x="5" y="-72 db" text="graph.units.s" halign="1" valign="1" color="graph_prim"/>
						<ui:with y="-24 db" halign="1" valign="1" color="graph_prim">
							<text x="0" text="0" halign="-1"/>
							<text x="1" text="1"/>
							<text x="2" text="2"/>
							<text x="3" text="3"/>
							<text x="4" text="4"/>
							<text x="5" text="graph.units.s"/>
						</ui:with>
						<ui:with x="5" halign="1" valign="1" color="graph_prim">
							<text y="12 db" text="graph.units.db" text:value="0" valign="-1"/>
							<ui:for id="v" first="-18" last="6" step="6">
								<text y="${v} db" text="${v}"/>
							</ui:for>
						</ui:with>
					</graph>

					<grid rows="3" cols="3" hfill="false" hexpand="false" hspacing="4">
						<ui:with bg.color="bg_schema">
							<label text="labels.gain_" padding="4"/>
							<label text="labels.chan.in" padding="4"/>
							<label text="labels.chan.out" padding="4"/>

							<grid rows="2" cols="2" fill="false" hspacing="2">
								<button id="grg_l" size="10" ui:inject="Button_blue" led="true"/>
								<button id="grg_r" size="10" ui:inject="Button_blue" led="true"/>
							</grid>

							<grid rows="2" cols="2" fill="false" hspacing="2">
								<button id="ilg_l" size="10" ui:inject="Button_left" led="true"/>
								<button id="ilg_r" size="10" ui:inject="Button_right" led="true"/>
							</grid>

							<grid rows="2" cols="2" fill="false" hspacing="2">
								<button id="olg_l" size="10" ui:inject="Button_left" led="true"/>
								<button id="olg_r" size="10" ui:inject="Button_right" led="true"/>
							</grid>
						</ui:with>

						<!-- Band gain reduction meter -->
						<ledmeter vexpand="true" angle="1">
							<ledchannel id="grm_l" min="-24 db" max="12 db" log="true" balance="0 db" balance.visibility="true" type="peak" value.color="blue" activity=":oclip and :grg_l"/>
							<ledchannel id="grm_r" min="-24 db" max="12 db" log="true" balance="0 db" balance.visibility="true" type="peak" value.color="blue" activity=":oclip and :grg_r"/>
						</ledmeter>
						<!-- Band input level -->
						<ledmeter vexpand="true" angle="1">
							<ledchannel id="ilm_l" min="-60 db" max="12 db" log="true" type="rms_peak" peak.visibility="true" value.color="left_in" activity=":oclip and :ilg_l"/>
							<ledchannel id="ilm_r" min="-60 db" max="12 db" log="true" type="rms_peak" peak.visibility="true" value.color="right_in" activity=":oclip and :ilg_r"/>
						</ledmeter>
						<!-- Band output level -->
						<ledmeter vexpand="true" angle="1">
							<ledchannel id="olm_l" min="-60 db" max="12 db" log="true" type="rms_peak" peak.visibility="true" value.color="left" activity=":oclip and :olg_l"/>
							<ledchannel id="olm_r" min="-60 db" max="12 db" log="true" type="rms_peak" peak.visibility="true" value.color="right" activity=":oclip and :olg_r"/>
						</ledmeter>
					</grid>
				</hbox>
			</vbox>
		</tabs>
	</vbox>
</plugin>
